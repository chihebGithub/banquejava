  </servlet>
  <servlet>
  	<servlet-name>CompteController</servlet-name>
  	<display-name>CompteController</display-name>
  	<description></description>
  	<servlet-class>com.banque.controllers.CompteController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ClientController</servlet-name>
  	<url-pattern>/ClientController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddClient</servlet-name>
  	<url-pattern>/AddClient</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CompteController</servlet-name>
  	<url-pattern>/CompteController</url-pattern>
  </servlet-mapping>
